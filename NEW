local MainSection = MainTab:CreateSection("قرينفيل 🚘")
local MainSection = MainTab:CreateSection("[➕️] تم اضافه بدون لاق")
Rayfield:Notify({
   Title = " منور يا حب ❤️",
   Content = "👑 انت تمتلك النسخه الاسطورية 👑",
   Duration = 0,
   Image = 13047715178,
   Actions = { -- Notification Buttons
      Ignore = {
         Name = "شكراً",
         Callback = function()
         print("لا تسوي ذكي عشان تسرق معلومات الهاك 🤓☝️")
      end
   },
},
})

